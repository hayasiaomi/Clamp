                <TextBlock Grid.Column="0" x:Name="MainWindowTitle" VerticalAlignment="Center" Text="{ext:Localize Activtied_Title }" Margin="20,0,0,0" FontSize="18" Foreground="#FFFFFF" />

                <Button Grid.Column="1"  Background="Transparent" VerticalAlignment="Center" Visibility="Visible"  HorizontalAlignment="Right" Margin="0,0,20,0" Click="BtnProxy_Click" Style="{StaticResource SystemButtonNoShadow }">
